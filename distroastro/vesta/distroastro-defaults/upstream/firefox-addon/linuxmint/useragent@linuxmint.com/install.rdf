<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">

    <!-- Addon -->
    <em:id>useragent@linuxmint.com</em:id>
    <em:name>Mint User Agent</em:name>
    <em:description>Set the user agent string to Linux Mint.</em:description>
    <em:version>1.0</em:version>
    <em:creator>Bamm Gabriana</em:creator>
    <em:iconURL>chrome://linuxmint/content/linuxmint.png</em:iconURL>
    <em:homepageURL>http://www.linuxmint.com/</em:homepageURL>
    <em:type>2</em:type>

    <!-- Mozilla Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>4.0</em:minVersion>
        <em:maxVersion>99.*</em:maxVersion>
      </Description>
    </em:targetApplication>

  </Description>
</RDF>
