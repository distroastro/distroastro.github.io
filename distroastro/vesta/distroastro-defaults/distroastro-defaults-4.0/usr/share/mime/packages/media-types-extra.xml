	<!-- Metadata -->
	<mime-type type="application/xmp+xml">
		<comment>XMP Metadata</comment>
		<acronym>XMP</acronym>
		<expanded-acronym>Extensible Metadata Platform</expanded-acronym>
		<sub-class-of type="application/xml"/>
		<glob pattern="*.xmp"/>
		<alias type="text/xmp+xml"/>
	</mime-type>

	<!-- Fax Formats -->
	<mime-type type="image/fax-g3">
		<comment>CCITT G3 fax</comment>
		<glob pattern="*.g3"/>
		<glob pattern="*.301"/>
		<glob pattern="*.brk"/>
	</mime-type>
	<mime-type type="image/fax-g4">
		<comment>CCITT G4 fax</comment>
		<glob pattern="*.g4"/>
		<glob pattern="*.ncr"/>
		<glob pattern="*.kfx"/>
	</mime-type>
	<mime-type type="image/vnd.ms-fax">
		<comment>Microsoft Fax Document</comment>
		<acronym>AWD</acronym>
		<expanded-acronym>At Work Document</expanded-acronym>
		<glob pattern="*.awd"/>
	</mime-type>

	<!-- Sound and Video Formats -->
	<mime-type type="video/x-pvr">
		<acronym>PVR</acronym>
		<expanded-acronym>Personal Video Recorder</expanded-acronym>
		<glob pattern="*.pvr"/>
	</mime-type>

	<!-- Microscopy -->
	<mime-type type="image/x-ids">
		<comment>Huygens microscopy image</comment>
		<glob pattern="*.ids"/>
		<glob pattern="*.ics"/>
		<glob pattern="*.ics2"/>
	</mime-type>

	<!-- Models -->
	<mime-type type="model/x-b3d">
		<comment>Blitz3D entity model</comment>
		<generic-icon name="image-x-generic"/>
		<glob pattern="*.b3d"/>
		<alias type="model/vnd.blitz.b3d"/>
	</mime-type>

	<!-- Game Data -->
	<mime-type type="image/x-eff">
		<comment>EverQuest image</comment>
		<acronym>EFF</acronym>
		<expanded-acronym>EverQuest Effects File</expanded-acronym>
		<glob pattern="*.eff"/>
