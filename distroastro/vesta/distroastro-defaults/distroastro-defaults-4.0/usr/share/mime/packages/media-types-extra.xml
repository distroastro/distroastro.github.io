		<generic-icon name="image-x-generic"/>
		<glob pattern="*.cut"/>
		<alias type="application/x-cut"/>
		<alias type="application/cut"/>
		<alias type="image/x-halo-cut"/>
		<alias type="image/cut"/>
	</mime-type>
	<mime-type type="application/x-ms-iconlibrary">
